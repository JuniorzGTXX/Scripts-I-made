Tab2:Toggle({
	Name = "Speed Up All Carts",
	Description = nil,
	Callback = function(Value)
        _G.Destroy = (Value)
        while _G.Destroy do
        wait(0.1)
        for i,v in pairs(game.Workspace.hey:GetChildren()) do
               fireclickdetector(v.Up.Click)
            end
            end
        end,
	Enabled = false
}) --Speed up all carts

